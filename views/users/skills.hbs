                                    <th>Logiciel</th>
                                    <th style="min-width: 130px;">Mon niveau</th>
                                </tr>
                            </thead>
                            <tbody>
                            {{#each user.candidate.softwares as |software|}}
                                <tr>
                                    <td>{{software.name}}</td>
                                    <td>
