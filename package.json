{
  "name": "mstaff",
  "version": "2.0.5",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --cache .",
    "start": "npm run lint && gulp build && node ./bin/www",
    "debug:express": "DEBUG=express:* node ./bin/www",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "dev": "nodemon --exec npm start",
    "test": "ENV=test mocha tests/*/*.js --exit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "build": "npm run lint && gulp build",
    "inspect": "node --inspect ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sorok-Dva/mstaff"
  },
