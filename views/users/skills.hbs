                                    <th>Matériel</th>
                                    <th style="min-width: 130px;">Mon niveau</th>
                                </tr>
                            </thead>
                            <tbody>
                            {{#each user.candidate.equipments as |equipment|}}
                                <tr>
                                    <td>{{equipment.name}}</td>
                                    <td>
                                        <span class="fa fa-star checked"></span>
                                        <span class="fa fa-star checked"></span>
