<div class="sidebar" data-background-color="white" data-active-color="success">
  <!--
      Tip 1: you can change the color of the sidebar's background using: data-background-color="white | brown"
      Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
  -->
  <div class="logo">
    <a href="#" class="simple-text logo-mini">
      MS
    </a>
    <a href="#" class="simple-text logo-normal">
      Mstaff
    </a>
  </div>
  <div class="sidebar-wrapper">
    <ul class="nav">
      <li{{#ifCond a.main '===' 'dashboard'}} class="active"{{/ifCond}}>
        <a data-toggle="collapse" href="#dashboardOverview"
           aria-expanded="{{#ifCond a.main '===' 'dashboard'}}true{{else}}false{{/ifCond}}">
          <i class="ti-panel"></i>
          <p>Dashboard
            <b class="caret"></b>
          </p>
        </a>
        <div class="collapse{{#ifCond a.main '===' 'dashboard'}} in{{/ifCond}}" id="dashboardOverview">
          <ul class="nav">
            <li{{#ifCond a.sub '===' 'overview'}} class="active"{{/ifCond}}>
              <a href="/back-office/">
                <span class="sidebar-mini"><i class="far fa-eye"></i></span>
                <span class="sidebar-normal">Overview</span>
              </a>
            </li>
            <li{{#ifCond a.sub '===' 'stats'}} class="active"{{/ifCond}}>
              <a href="/back-office/stats">
                <span class="sidebar-mini"><i class="fal fa-chart-line"></i></span>
                <span class="sidebar-normal">Stats</span>
              </a>
            </li>
          </ul>
        </div>
      </li>
      <li{{#ifCond a.main '===' 'users'}} class="active"{{/ifCond}}>
        <a data-toggle="collapse" href="#usersOverview"
           aria-expanded="{{#ifCond a.main '===' 'users'}}true{{else}}false{{/ifCond}}">
          <i class="fal fa-users"></i>
          <p>Utilisateurs
            <b class="caret"></b>
          </p>
        </a>
        <div class="collapse{{#ifCond a.main '===' 'users'}} in{{/ifCond}}" id="usersOverview">
          <ul class="nav">
            <li{{#ifCond a.sub '===' 'users_all'}} class="active"{{/ifCond}}>
              <a href="/back-office/users/">
                <span class="sidebar-mini"><i class="fal fa-th-list"></i></span>
                <span class="sidebar-normal">Liste complète</span>
              </a>
            </li>
            <li{{#ifCond a.sub '===' 'candidates'}} class="active"{{/ifCond}}>
              <a href="/back-office/users/candidates">
                <span class="sidebar-mini"><i class="fal fa-user-md"></i></span>
                <span class="sidebar-normal">Candidats</span>
              </a>
            </li>
            <li{{#ifCond a.sub '===' 'es'}} class="active"{{/ifCond}}>
              <a href="/back-office/users/es">
                <span class="sidebar-mini"><i class="fal fa-hospitals"></i></span>
                <span class="sidebar-normal">ES</span>
              </a>
            </li>
            {{#ifCond a.sub '===' 'profile'}}
              <li class="active">
                <a href="#">
                  <span class="sidebar-mini"><i class="fal fa-id-card-alt"></i></span>
                  <span class="sidebar-normal">Profil d'un utilisateur</span>
                </a>
              </li>
            {{/ifCond}}
          </ul>
        </div>
      </li>
      <li{{#ifCond a.main '===' 'es'}} class="active"{{/ifCond}}>
        <a data-toggle="collapse" href="#esOverview"
           aria-expanded="{{#ifCond a.main '===' 'es'}}true{{else}}false{{/ifCond}}">
          <i class="fal fa-hospitals"></i>
          <p>Établissements Mstaff
            <b class="caret"></b>
          </p>
        </a>
        <div class="collapse{{#ifCond a.main '===' 'es'}} in{{/ifCond}}" id="esOverview">
          <ul class="nav">
            <li{{#ifCond a.sub '===' 'es_all'}} class="active"{{/ifCond}}>
              <a href="/back-office/es/">
                <span class="sidebar-mini"><i class="fal fa-th-list"></i></span>
                <span class="sidebar-normal">Liste complète</span>
              </a>
            </li>
            {{#ifCond a.sub '===' 'view'}}
              <li class="active">
                <a href="#">
                  <span class="sidebar-mini"><i class="fal fa-eye"></i></span>
                  <span class="sidebar-normal">Vue d'un établissement</span>
                </a>
              </li>
            {{/ifCond}}
          </ul>
        </div>
      </li>
        <li{{#ifCond a.main '===' 'references'}} class="active"{{/ifCond}}>
            <a data-toggle="collapse" href="#referencesOverview"
               aria-expanded="{{#ifCond a.main '===' 'references'}}true{{else}}false{{/ifCond}}">
                <i class="ti-user"></i>
                <p>Références
                    <b class="caret"></b>
                </p>
            </a>
            <div class="collapse{{#ifCond a.main '===' 'references'}} in{{/ifCond}}" id="referencesOverview">
                <ul class="nav">
                    <li{{#ifCond a.sub '===' 'formations'}} class="active"{{/ifCond}}>
                        <a href="/back-office/formations">
                            <span class="sidebar-mini">F</span>
                            <span class="sidebar-normal">Formations</span>
                        </a>
                    </li>
                </ul>
            </div>
        </li>
    </ul>
  </div>
</div>
