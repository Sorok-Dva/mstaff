<div class="modal-body">
  <div class="form-group">
    <label for="groupValueEdit"> Nom du groupe </label>
    <input class="form-control" type="text" id="groupValueEdit" value="{{this.group.name}}">
  </div>
  <div class="form-group">
    <label for="groupLogoEdit">Logo</label>
    <input class="form-control" type="text" id="groupLogoEdit" value="{{this.group.logo}}">
  </div>
  <div class="form-group">
    <label for="groupBannerEdit">Bannière</label>
    <input class="form-control" type="text" id="groupBannerEdit" value="{{this.group.banner}}">
  </div>
  <div class="form-group">
    <label for="groupDomainEdit">Domaine</label>
    <select class="form-control" id="groupDomainEdit" name="domaine_enable">
      <option value="0" {{#ifCond this.supergroup.domain_enable '===' 'Inactif'}}selected="selected"{{/ifCond}}>Inactif</option>
      <option value="1" {{#ifCond this.supergroup.domain_enable '===' 'Actif'}}selected="selected"{{/ifCond}}>Actif</option>
    </select>
  </div>
  <div class="form-group">
    <label for="groupUrlEdit">Nom de domaine</label>
    <div class="row">
      <div class="col-md-8">
        <input class="form-control" placeholder="établissement" id="groupUrlEdit" name="domain_name" type="text" value="{{this.group.domain_name}}">
      </div>
      <div class="col-md-4" style="margin-left: -26px;"><h4>.mstaff.co</h4></div>
    </div>
  </div>
</div>
<div class="modal-footer">
  <button type="butto1n" class="btn btn-default" id="btnEditGroup"> Modifier le groupe</button>
</div>