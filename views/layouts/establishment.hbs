      <h1 class="text-center" style="color:white;">{{es.name}}</h1>
    </div>
  </div>

  <div class="wrapped">
    <div class="container-fluid">
      {{> header}}
      <div id="bodyContent" class="col-md-12 content">
        <div class="container-fluid">
          {{> flash}}
          {{{body}}}
        </div>
      </div>
    </div>
    <input id="csrfToken" type="text" value="{{csrfToken}}" class="hidden">
  </div>

